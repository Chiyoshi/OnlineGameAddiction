<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<properties>
		<property name="db.url" value="${db.url}" />
		<!-- <property name="db.username" value="${db.username}" /> -->
		<!-- <property name="db.password" value="${db.password}" /> -->
	</properties>
	<settings>
		<setting name="logImpl" value="LOG4J2" />
	</settings>
	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="org.sqlite.JDBC" />
				<property name="url" value="${db.url}" />
				<!-- <property name="username" value="${db.username}" /> -->
				<!-- <property name="password" value="${db.password}" /> -->
			</dataSource>
		</environment>
	</environments>
	<mappers>
		<mapper class="com.oga.app.dataaccess.dao.mapper.DailyRewardHistoryMapper" />
		<mapper class="com.oga.app.dataaccess.dao.mapper.LoginCampaignDetailsMapper" />
		<mapper class="com.oga.app.dataaccess.dao.mapper.LoginCampaignHistoryMapper" />
<!-- 		<mapper class="com.oga.app.dataaccess.dao.mapper.OperationLogMapper" /> -->
		<mapper class="com.oga.app.dataaccess.dao.mapper.RouletteHistoryMapper" />
		<mapper class="com.oga.app.dataaccess.dao.mapper.RSManagementMapper" />
		<mapper class="com.oga.app.dataaccess.dao.mapper.UserMapper" />
	</mappers>
</configuration>
